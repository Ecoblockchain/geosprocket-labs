  <iati-activity xml:lang="en" default-currency="XDR" hierarchy="1" last-updated-datetime="2013-06-28T15:42:51">
    <iati-identifier>46002-P-RW-AAE-006</iati-identifier>
    <reporting-org ref="46002" type="40" xml:lang="en">African Development Bank Group</reporting-org>
